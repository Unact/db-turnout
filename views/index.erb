<% if res %>
<table>
	<thead>
		<tr>
			<% res.columns.each do |column| %>
			<th>
				<%= column %>
			</th>
			<% end %>
		</tr>
	</thead>
	<tbody>
		<% res.rows.each do |row| %>
		<tr>
			<% row.each do |v| %>
			<td>
				<%= v %>
			</td>
			<% end %>
		</tr>
		<% end %>
	</tbody>
</table>
<% end %>
<%= extra %>
